Cell[BoxData[
 RowBox[{
  RowBox[{"normalizeGraph", "[", "expr_", "]"}], ":=", "\[IndentingNewLine]", 
  
  RowBox[{"expr", "/.", "\[VeryThinSpace]", 
   RowBox[{"{", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{
      RowBox[{
       RowBox[{"g_Graph", "?", "GraphQ"}], "/;", 
       RowBox[{
        RowBox[{"VertexList", "[", "g", "]"}], "===", 
        RowBox[{"VertexList", "@", 
         RowBox[{"EdgeList", "[", "g", "]"}]}]}]}], "\[RuleDelayed]", 
      "\[IndentingNewLine]", 
      RowBox[{"With", "[", 
       RowBox[{
        RowBox[{"{", 
         RowBox[{"e", "=", 
          RowBox[{
           RowBox[{"EdgeList", "[", "g", "]"}], "/.", "\[VeryThinSpace]", 
           RowBox[{"{", 
            RowBox[{
             RowBox[{"UndirectedEdge", "\[Rule]", "TwoWayRule"}], ",", 
             RowBox[{"DirectedEdge", "\[Rule]", "Rule"}]}], "}"}]}]}], "}"}], 
        ",", 
        RowBox[{
         RowBox[{"Graph", "[", "e", "]"}], "/;", "True"}]}], "]"}]}], ",", 
     "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"g_Graph", "?", "GraphQ"}], "\[RuleDelayed]", 
      "\[IndentingNewLine]", 
      RowBox[{"With", "[", 
       RowBox[{
        RowBox[{"{", 
         RowBox[{
          RowBox[{"v", "=", 
           RowBox[{"VertexList", "[", "g", "]"}]}], ",", 
          RowBox[{"e", "=", 
           RowBox[{
            RowBox[{"EdgeList", "[", "g", "]"}], "/.", "\[VeryThinSpace]", 
            RowBox[{"{", 
             RowBox[{
              RowBox[{"UndirectedEdge", "\[Rule]", "TwoWayRule"}], ",", 
              RowBox[{"DirectedEdge", "\[Rule]", "Rule"}]}], "}"}]}]}]}], 
         "}"}], ",", 
        RowBox[{
         RowBox[{"Graph", "[", 
          RowBox[{"v", ",", "e"}], "]"}], "/;", "True"}]}], "]"}]}]}], 
    "\[IndentingNewLine]", "}"}]}]}]], "Input",
 InitializationGroup->True,
