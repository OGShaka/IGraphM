        RowBox[{"normalizeGraph", "@", 
         RowBox[{"Hold", "[", "input", "]"}]}]}], ",", "\"\<,\\n\>\"", ",", 
       "\[IndentingNewLine]", "\"\<  \>\"", ",", 
       RowBox[{"ts", "@@", 
        RowBox[{"normalizeGraph", "@", 
         RowBox[{"Hold", "[", "res", "]"}]}]}], ",", "\[IndentingNewLine]", 
       RowBox[{"If", "[", 
        RowBox[{
         RowBox[{"GraphQ", "[", "res", "]"}], ",", 
         "\"\<,\\n  SameTest -> IGSameGraphQ\>\"", ",", "\"\<\>\""}], "]"}], 
       ",", "\[IndentingNewLine]", 
